parameters:
	bootstrap: tests/bootstrap.php
	ignoreErrors:
	    - '#Parameter .* Mockery\\MockInterface given.#'
	    - '#Call to an undefined method Mockery\\ExpectationInterface|Mockery\\HigherOrderMessage::andReturn().#'
	    - '#Call to an undefined method DevboardLib\\GitHubApi\\Auth\\GitHubApp\\JwtTokenBuilder|Mockery\\MockInterface::shouldReceive().#'
	    - '#Parameter \#1 \$json of function json_decode expects string, string|false given.#'
	includes:
	    - vendor/phpstan/phpstan-phpunit/extension.neon
	    - vendor/phpstan/phpstan-phpunit/rules.neon





