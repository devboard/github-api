{
    "name": "devboard/github-api",
    "description": "Devboard lib for GitHub API",
    "type": "project",
    "license": "proprietary",
    "repositories": [
        {
            "type": "vcs",
            "comment": "Adding my own multiline fixers",
            "url": "https://github.com/msvrtan/PHP-CS-Fixer.git"
        }
    ],
    "require": {
        "php": "^7.1",
        "devboard/lib-github": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "knplabs/github-api": "^2.8",
        "lcobucci/jwt": "^3.2",
        "myclabs/php-enum": "^1.5",
        "php-http/guzzle6-adapter": "^1.1",
        "webmozart/assert": "^1.2"
    },
    "require-dev": {
        "ciaranmcnulty/phpspec-typehintedmethods": "^3.0",
        "devboard/thesting-data-source": "^1.1",
        "friendsofphp/php-cs-fixer": "dev-long-line-fixers2",
        "infection/infection": "^0.8.1",
        "mockery/mockery": "^1.0",
        "phing/phing": "^2.16",
        "phpmd/phpmd": "^2.6",
        "phpspec/phpspec": "^4.2",
        "phpstan/phpstan": "^0.9.0",
        "phpstan/phpstan-phpunit": "^0.9.0",
        "phpunit/phpunit": "^7.0",
        "slevomat/coding-standard": "^4.0",
        "squizlabs/php_codesniffer": "^3.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "DevboardLib\\GitHubApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\DevboardLib\\GitHubApi\\": "spec/",
            "Tests\\DevboardLib\\GitHubApi\\": "tests/"
        }
    }
}
